# Conversion to libvirt XML

# Install libvirt
	pacman -Syu libvirt

# Enable correct services
	systemctl enable virtlockd.socket virtlogd.socket libvirtd.service
	systemctl start virtlockd.socket virtlogd.socket libvirtd.service

# Convert qemu to libvirt xml
	sudo virsh domxml-from-native qemu-argv test.sh