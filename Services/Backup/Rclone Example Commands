rclone copy \
-v \
-P \
--fast-list \
--transfers=20 \
--checkers=20 \
--drive-chunk-size=1M \
--log-file /home/fileserver/Applications/rclone/rclone.log \
--filter-from /home/fileserver/Applications/rclone/rclone_exclusions \
/home/fileserver/Media/TVShows gsuite:TVShows

--bwlimit "12:00,1b 23:59,off" \

rclone sync \
-v \
-P \
--fast-list \
--transfers=20 \
--checkers=20 \
--drive-chunk-size=1M \
--log-file /home/fileserver/Applications/rclone/rclone.log \
--filter-from /home/fileserver/Applications/ArchServer/config/rclone_exclusions \
/home/fileserver/Media/Games /home/fileserver/Backup/Games
